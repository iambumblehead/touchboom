{
  "name": "touchboom",
  "main": "src/touchboom",
  "version": "0.2.49",
  "readmeFilename": "README.md",
  "license": "MIT",
  "description": "Report touch/mouse events from the document",
  "repository": {
    "type": "git",
    "url": "https://github.com/iambumblehead/touchboom.git"
  },
  "dependencies": {
    "nodefocusable": "^0.0.1",
    "evdelegate": "^0.0.3",
    "curved": "0.0.7",
    "domev": "0.0.7"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "scroungejs": "^1.3.0",
    "array-flatten": "^2.1.1",
    "express": "~4.13.x",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "babel-eslint": "^7.2.3",
    "pre-commit": "^1.2.2",
    "eslint-plugin-import": "^2.7.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "start": "node build.js && node express.js",
    "build": "./node_modules/babel-cli/bin/babel.js --presets env -d dist/ src/touchboom.js"
  },
  "pre-commit": {
    "silent": true,
    "run": [
      "lint"
    ]
  },
  "keywords": [
    "inertia",
    "glide",
    "touch",
    "mouse",
    "tap",
    "doubleclick"
  ]
}
